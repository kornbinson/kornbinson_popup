{
    "name":"Youtube notificator",
    "version":"1.0",
    "description":"Utilizalo para enterarte de nuevos videos",
    "manifest_version":2,
    "author":"Izan",
    "icons":{
            "16":"src/icon16.png",            
            "48":"src/icon48.png",
            "128":"src/icon128.png"
    },
    "browser_action":{
        "default_title":"Youtube notificador",
        "default_icon":"src/icon19.png",
        "default_popup":"popup.html"
    },
    "background":{
        "page":"src/background.html"
    },
    "permissions":[
        "cookies",
        "webRequest",
        "webRequestBlocking",
        "notifications",        
        "tabs",
        "storage",
        "http://*/*","https://*/*",
        "https://www.youtube.com/*",
        "http://www.youtube.com/*"            
    ],
    "web_accessible_resources":[
        "src/icon48.png"
    ]
}